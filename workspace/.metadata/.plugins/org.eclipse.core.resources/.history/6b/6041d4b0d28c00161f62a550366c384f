#include "header.h"


void fft_function()
{
	const uint8_t ifft = FALSE;
	const uint8_t bit_reverse = FALSE;
	arm_cfft_radix2_instance_q31 fft_inst_31;
	arm_status status;

	status = arm_cfft_radix2_init_q31(&fft_inst_31, FFT_SIZE, ifft, bit_reverse);
	arm_cfft_radix2_q31(, ADC_samples, sample_length);
	arm_cmplx_mag_q15(ADC_samples, MAG_of_fft, fftlength);
}
